# Example .env file for ai_dm_voice_app
OPENAI_API_KEY=your-openai-api-key-here
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here
DISCORD_BOT_TOKEN=your-discord-bot-token-here
DEV_GUILD_ID=123456789012345678  # Set to your test server's ID for dev sync, or leave blank for global

# TTS Provider: 'elevenlabs' (default), 'edge' (free), or 'disabled'
TTS_PROVIDER=elevenlabs

# Flask secret key for session management (generate a random string for production)
FLASK_SECRET_KEY=change-this-to-a-random-secret-key
